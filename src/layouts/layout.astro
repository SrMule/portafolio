---
interface Props {
	Title: string;
}

const Title = Astro.props as Props;
---

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{Title}</title>
</head>
<body>
    <header></header>
    <slot/>
    <footer></footer>
</body>
</html>